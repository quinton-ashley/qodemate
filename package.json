{
  "author": "quinton-ashley",
  "bin": {
    "qodemate": "./app.js"
  },
  "bugs": {
    "url": "https://github.com/quinton-ashley/qodemate/issues"
  },
  "build": {
    "appId": "com.qashto.qodemate",
    "copyright": "Copyright Â© 2019 Quinton Ashley",
    "electronVersion": "8.1.1",
    "electronDownload": {
      "version": "8.1.1"
    },
    "mac": {
      "category": "public.app-category.education",
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    }
  },
  "contributors": [
    {
      "name": "Quinton Ashley",
      "email": "qashto@gmail.com"
    }
  ],
  "dependencies": {
    "await-spawn": "^3.0.0",
    "bootstrap": "^4.4.1",
    "contro": "^2.1.3",
    "contro-ui": "^2.0.11",
    "copy-paste": "^1.3.0",
    "csv-string": "^3.2.0",
    "deep-extend": "^0.6.0",
    "delay": "^4.3.0",
    "electron-pug": "^2.0.0",
    "electron-updater": "^4.2.5",
    "fs-extra": "^9.0.0",
    "get-open-apps": "^1.0.8",
    "gitignore-globs": "^0.1.1",
    "gitignore-templates": "0.0.6",
    "ignore": "^5.1.4",
    "jquery": "^3.4.1",
    "klaw": "^3.0.0",
    "markdown-it": "^10.0.0",
    "material-design-icons-iconfont": "^5.0.1",
    "minimist": "^1.2.5",
    "moment": "^2.24.0",
    "mousetrap": "^1.6.5",
    "open": "^7.0.3",
    "p5": "^1.0.0",
    "popper.js": "^1.16.1",
    "pug": "^2.0.4",
    "robotjs": "^0.6.0",
    "tether": "^1.4.7"
  },
  "description": "Qodemate is a dynamic, un-sandboxed CS presentation/textbook platform!",
  "devDependencies": {},
  "entry point": "app.js",
  "keywords": [
    "app"
  ],
  "license": "UNLICENSED",
  "main": "app.js",
  "maintainers": [
    {
      "name": "Quinton Ashley",
      "email": "qashto@gmail.com"
    }
  ],
  "name": "qodemate",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quinton-ashley/qodemate.git"
  },
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "up-outdated": "npm install --save $(npm outdated | cut -d ' ' -f -1 | sed '1d' | sed 's/$/@*/')",
    "rebuild": "npm rebuild --build-from-source --runtime=electron --target=8.1.1 --disturl=https://atom.io/download/atom-shell --abi=75",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -m && electron-builder -w && electron-builder -l",
    "gh-publish": "electron-builder -m -l -w -p always",
    "postinstall": "electron-builder install-app-deps -c.electronVersion 8.1.1 | electron-rebuild",
    "delete-tags": "git tag | xargs git tag -d",
    "delete-tags-remote": "git tag -l | xargs -n 1 git push --delete origin",
    "v": "npm version patch --force",
    "version": "git add -A",
    "postversion": "git push",
    "V": "npm version minor --force"
  },
  "version": "0.1.29"
}
